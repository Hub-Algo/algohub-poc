{
  "name": "algohub_poc_smart_contracts",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "generate-campaign": "algokitgen generate -a contracts/artifacts/campaign/Campaign.json -o contracts/clients/CampaignClient.ts",
    "generate-voters": "algokitgen generate -a contracts/artifacts/voters/Voters.json -o contracts/clients/VotersClient.ts",
    "generate-clients": "npm run generate-campaign && npm run generate-voters",
    "generate-campaign-comp": "algokit-generate-component contracts/artifacts/campaign/Campaign.json contracts/artifacts/campaign/components",
    "generate-voters-comp": "algokit-generate-component contracts/artifacts/voters/Voters.json contracts/artifacts/voters/components",
    "generate-components": "npm run generate-campaign-comp && npm run generate-voters-comp",
    "compile-campaign": "tealscript contracts/campaign.algo.ts contracts/artifacts/campaign",
    "compile-voters": "tealscript contracts/voters.algo.ts contracts/artifacts/voters",
    "build": "npm run compile-campaign && npm run compile-voters && npm run generate-clients",
    "test": "npm run build && jest",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^2.2.0",
    "algosdk": "^2.4.0"
  },
  "devDependencies": {
    "@algorandfoundation/algokit-client-generator": "^2.2.1",
    "@algorandfoundation/tealscript": "^0.59.0",
    "@jest/globals": "^29.5.0",
    "@joe-p/algokit-generate-component": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.0",
    "typescript": "5.0.2"
  }
}
